{
  "manifest_version": 3,
  "name": "Unconsent",
  "description": "Remove consent popups from almost all websites without consenting to cookies.",
  "version": "1.0",
  "action": {
    "default_popup": "unconsent.html"
  },
  "icons": {
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["unconsent.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ]
}
